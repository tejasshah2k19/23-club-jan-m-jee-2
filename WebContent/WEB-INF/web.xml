<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>23-jan-m-2</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>SignupServlet</servlet-name>
    <servlet-class>com.controller.SignupServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SignupServlet</servlet-name>
    <url-pattern>/SignupServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RegistrationServlet</servlet-name>
    <servlet-class>com.controller.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegistrationServlet</servlet-name>
    <url-pattern>/RegistrationServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SaveDataSesssionServlet</display-name>
    <servlet-name>SaveDataSesssionServlet</servlet-name>
    <servlet-class>com.controller.SaveDataSesssionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SaveDataSesssionServlet</servlet-name>
    <url-pattern>/SaveDataSesssionServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ReadDataFromSessionServlet</display-name>
    <servlet-name>ReadDataFromSessionServlet</servlet-name>
    <servlet-class>com.controller.ReadDataFromSessionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReadDataFromSessionServlet</servlet-name>
    <url-pattern>/ReadDataFromSessionServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LogoutServlet</display-name>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>2</session-timeout>
  </session-config>
  <listener>
    <listener-class>com.listener.UserCounterListener</listener-class>
  </listener>
  <servlet>
    <description></description>
    <display-name>FilterLoginServlet</display-name>
    <servlet-name>FilterLoginServlet</servlet-name>
    <servlet-class>com.controller.FilterLoginServlet</servlet-class>
    <init-param>
      <param-name>email</param-name>
      <param-value>royal@gmail.com</param-value>
    </init-param>
    <init-param>
      <param-name>password</param-name>
      <param-value>royal1234</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>FilterLoginServlet</servlet-name>
    <url-pattern>/FilterLoginServlet</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>LoginFilter</filter-name>
    <filter-class>com.filter.LoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LoginFilter</filter-name>
    <url-pattern>/FilterLoginServlet</url-pattern>
  </filter-mapping>
  <context-param>
    <param-name>adminName</param-name>
    <param-value>Admin</param-value>
  </context-param>
  <servlet>
    <description></description>
    <display-name>DataServlet</display-name>
    <servlet-name>DataServlet</servlet-name>
    <servlet-class>com.controller.DataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DataServlet</servlet-name>
    <url-pattern>/DataServlet</url-pattern>
  </servlet-mapping>
  <jsp-config>
  	<taglib>
  		<taglib-uri>www.royal.com/faculty/stag/tejas</taglib-uri>
  		<taglib-location>/WEB-INF/stag.tld</taglib-location>
  	</taglib>
  	<taglib>
  		<taglib-uri>www.royal.com/faculty/ctag/tejas</taglib-uri>
  		<taglib-location>/WEB-INF/ctag.tld</taglib-location>
  	</taglib>
  	
  </jsp-config>
  
</web-app>